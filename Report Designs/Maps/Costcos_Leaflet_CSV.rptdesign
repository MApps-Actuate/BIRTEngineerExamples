        <text id="378">
            <property name="contentType">html</property>
            <text-property name="content"><![CDATA[<script>
	// Load Leaflet CSS
	var fr=document.createElement("link");
	fr.setAttribute("rel", "stylesheet");
	fr.setAttribute("type", "text/css");
	fr.setAttribute("href", "http://cdn.leafletjs.com/leaflet-0.5/leaflet.css");
	document.getElementsByTagName("head")[0].appendChild(fr);
	
	// Load Leaflet JavaScript
	var fr2=document.createElement('script');
	fr2.setAttribute("type","text/javascript");
	fr2.setAttribute("src", "http://cdn.leafletjs.com/leaflet-0.5/leaflet.js");
	document.getElementsByTagName("head")[0].appendChild(fr2);
	

birtleafletmaps = {
	divName: "birt-leaflet-map",
	locations: [],
	
	addLocationToMap : function(lat, lon) {
		var loc = new Object();
	    	loc.lat = lat;
		    loc.lon = lon;
	    
	    this.locations.push(loc);  
	}
};

</script>
]]></text-property>
